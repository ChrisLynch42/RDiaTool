<% 
    _erbout << 'class ChangeColumnsOf'
    _erbout << template_variables['table_name'].capitalize
    _erbout << "\n"
    _erbout << "  def self.up\n"
    template_variables['table_change'].modify().each { | key, value|
      if key.upcase() != 'ID'
        _erbout << "  change_column\n"
        _erbout << ' :'
        _erbout << template_variables['table_name']
        _erbout << ', :'
        _erbout << key
        _erbout << ', :'
        _erbout << value.data_type
      end

    }
    _erbout << "  end\n"
    _erbout << "end\n"
    _erbout << "\n"
    _erbout << "\n"
%>
