<% 
  @database_difference.create().each { | table_name, table_change | 
    _erbout << 'rails generate model '
    _erbout << table_name
    table_change.add().each { | key, value|
      if !value.references.nil? && value.references.length > 0
        _erbout << ' '
        _erbout << value.end_point.table_name
        _erbout << ':references'
      else
        _erbout << ' '
        _erbout << key
        _erbout << ':'
        _erbout << value.data_type
      end

    }
    _erbout << "\n"
  } 
%>
