<%
  _erbout << "  ###Do not edit the below \n"
  _erbout << "  ###Begin RDiaLib Code \n"
  attributes = '  attr_accessible '
  references = Hash.new()
  template_variables['database'].tables_by_name[template_variables['table_name']].columns().each do | key, value|
    if key.upcase() != 'ID'
      attributes << ' :' 
      attributes << key
      attributes << ', ' 
    end
  end
  _erbout << attributes[0..-3]
  _erbout << "\n"

  unless template_variables['belongs_to'][template_variables['table_name']].nil?
    template_variables['belongs_to'][template_variables['table_name']].each do | belongs_to |   
      _erbout << "  "
      _erbout << belongs_to    
      _erbout << "\n"
    end
  end

  unless template_variables['has_many'][template_variables['table_name']].nil?
    template_variables['has_many'][template_variables['table_name']].each do | has_many |
      _erbout << "  "
      _erbout << has_many    
      _erbout << "\n"
    end
  end

  unless template_variables['has_many_through'][template_variables['table_name']].nil?
    template_variables['has_many_through'][template_variables['table_name']].each do | has_many_through |
      _erbout << "  "
      _erbout << has_many_through    
      _erbout << "\n"
    end  
  end

  _erbout << "  ###Do not edit the above \n"
%>
